{
  "browser_specific_settings": {
    "gecko": {
      "id": "Ant1@ps0.dev"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "Ant1.js"
      ],
      "matches": [
        "https://businesswebmail.a1.net/webmail/*",
        "https://mail.a1.net/webmail/*"
      ]
    }
  ],
  "description": "A Firefox Add-on to combat tracking on websites powered by A1",
  "icons": {
    "16": "icons/Ant1-16.png",
    "32": "icons/Ant1-32.png",
    "64": "icons/Ant1-64.png",
    "128": "icons/Ant1-128.png"
  },
  "manifest_version": 2,
  "name": "Ant1",
  "version": "0.2"
}
